<script>
  document
    .getElementById("theme-button")
    .addEventListener("click", function () {
      var icon = document.getElementById("icon1");
      var icon = document.getElementById("icon2");

      document.querySelector(".main-menu-mobil").classList.toggle("hidden");
      document.querySelector("#icon2").classList.toggle("hidden");
      document.querySelector("#icon1").classList.toggle("hidden");
    });

  document
    .querySelectorAll(".menu-item-has-children")
    .forEach(function (menuItem) {
      menuItem.querySelector("a").addEventListener("click", function (event) {
        event.preventDefault();

        var menuInfo = document.querySelector(".menu-info");
        var menuName = document.querySelector(".menu-name");
        var plus = menuItem.querySelector(".plus");
        var minus = menuItem.querySelector(".minus");
        var dropdown = menuItem.querySelector(".mega-column-menu-4");

        plus.classList.toggle("hidden");
        minus.classList.toggle("hidden");
        dropdown.classList.toggle("hidden");

        menuItem.classList.toggle("active");
        // Assuming you want to toggle 'white-text' class only for elements within the clicked menuItem
        var menuName = menuItem.querySelector(".menu-name");
        var menuInfo = menuItem.querySelector(".menu-info");

        if (menuName) menuName.classList.toggle("white-text");
        if (menuInfo) menuInfo.classList.toggle("white-text");
      });
    });

  document.addEventListener("DOMContentLoaded", function () {
    const buttons = document.querySelectorAll(".button");
    buttons.forEach((button) => {
      const subMenu = button.nextElementSibling; // Assumes the sub-menu is right after the button

      subMenu.addEventListener("mouseenter", function () {
        button.classList.add("hover-persist");
      });

      subMenu.addEventListener("mouseleave", function () {
        button.classList.remove("hover-persist");
      });
    });
  });
</script>
